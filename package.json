  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://jkusa.github.io/ember-cli-clipboard/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
